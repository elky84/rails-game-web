<p id="notice"><%= notice %></p>

<h1>Listing Monster Enchant Bonus</h1>

<table>
  <thead>
    <tr>
      <th>Monster type</th>
      <th>Status id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @monster_enchant_bonus.each do |monster_enchant_bonu| %>
      <tr>
        <td><%= monster_enchant_bonu.MONSTER_TYPE %></td>
        <td><%= monster_enchant_bonu.STATUS_ID %></td>
        <td><%= link_to 'Show', monster_enchant_bonu %></td>
        <td><%= link_to 'Edit', edit_monster_enchant_bonu_path(monster_enchant_bonu) %></td>
        <td><%= link_to 'Destroy', monster_enchant_bonu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Monster enchant bonu', new_monster_enchant_bonu_path %>
