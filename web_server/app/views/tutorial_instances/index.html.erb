<p id="notice"><%= notice %></p>

<h1>Listing Tutorial Instances</h1>

<table>
  <thead>
    <tr>
      <th>Tutorial no</th>
      <th>User no</th>
      <th>Flow no</th>
      <th>Gacha get instance no</th>
      <th>Stage get instance no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutorial_instances.each do |tutorial_instance| %>
      <tr>
        <td><%= tutorial_instance.TUTORIAL_NO %></td>
        <td><%= tutorial_instance.USER_NO %></td>
        <td><%= tutorial_instance.FLOW_NO %></td>
        <td><%= tutorial_instance.GACHA_GET_INSTANCE_NO %></td>
        <td><%= tutorial_instance.STAGE_GET_INSTANCE_NO %></td>
        <td><%= link_to 'Show', tutorial_instance %></td>
        <td><%= link_to 'Edit', edit_tutorial_instance_path(tutorial_instance) %></td>
        <td><%= link_to 'Destroy', tutorial_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutorial instance', new_tutorial_instance_path %>
