<p id="notice"><%= notice %></p>

<h1>Listing Coupon Instances</h1>

<table>
  <thead>
    <tr>
      <th>Coupon index</th>
      <th>Reward</th>
      <th>Coupon string</th>
      <th>Server index</th>
      <th>User no</th>
      <th>Time</th>
      <th>Coupon group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupon_instances.each do |coupon_instance| %>
      <tr>
        <td><%= coupon_instance.COUPON_INDEX %></td>
        <td><%= coupon_instance.REWARD %></td>
        <td><%= coupon_instance.COUPON_STRING %></td>
        <td><%= coupon_instance.SERVER_INDEX %></td>
        <td><%= coupon_instance.USER_NO %></td>
        <td><%= coupon_instance.TIME %></td>
        <td><%= coupon_instance.COUPON_GROUP %></td>
        <td><%= link_to 'Show', coupon_instance %></td>
        <td><%= link_to 'Edit', edit_coupon_instance_path(coupon_instance) %></td>
        <td><%= link_to 'Destroy', coupon_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coupon instance', new_coupon_instance_path %>
