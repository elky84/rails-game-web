<p id="notice"><%= notice %></p>

<h1>Listing Log Instances</h1>

<table>
  <thead>
    <tr>
      <th>Log no</th>
      <th>User no</th>
      <th>Character no</th>
      <th>Log type</th>
      <th>Value</th>
      <th>Cash</th>
      <th>Money</th>
      <th>User lv</th>
      <th>Exp</th>
      <th>Action time</th>
      <th>Ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @log_instances.each do |log_instance| %>
      <tr>
        <td><%= log_instance.LOG_NO %></td>
        <td><%= log_instance.USER_NO %></td>
        <td><%= log_instance.CHARACTER_NO %></td>
        <td><%= log_instance.LOG_TYPE %></td>
        <td><%= log_instance.VALUE %></td>
        <td><%= log_instance.CASH %></td>
        <td><%= log_instance.MONEY %></td>
        <td><%= log_instance.USER_LV %></td>
        <td><%= log_instance.EXP %></td>
        <td><%= log_instance.ACTION_TIME %></td>
        <td><%= log_instance.IP %></td>
        <td><%= link_to 'Show', log_instance %></td>
        <td><%= link_to 'Edit', edit_log_instance_path(log_instance) %></td>
        <td><%= link_to 'Destroy', log_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log instance', new_log_instance_path %>
