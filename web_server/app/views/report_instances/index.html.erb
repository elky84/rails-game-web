<p id="notice"><%= notice %></p>

<h1>Listing Report Instances</h1>

<table>
  <thead>
    <tr>
      <th>Report no</th>
      <th>User no</th>
      <th>Character no</th>
      <th>Report type</th>
      <th>Value</th>
      <th>User lv</th>
      <th>Exp</th>
      <th>Action time</th>
      <th>Ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report_instances.each do |report_instance| %>
      <tr>
        <td><%= report_instance.REPORT_NO %></td>
        <td><%= report_instance.USER_NO %></td>
        <td><%= report_instance.CHARACTER_NO %></td>
        <td><%= report_instance.REPORT_TYPE %></td>
        <td><%= report_instance.VALUE %></td>
        <td><%= report_instance.USER_LV %></td>
        <td><%= report_instance.EXP %></td>
        <td><%= report_instance.ACTION_TIME %></td>
        <td><%= report_instance.IP %></td>
        <td><%= link_to 'Show', report_instance %></td>
        <td><%= link_to 'Edit', edit_report_instance_path(report_instance) %></td>
        <td><%= link_to 'Destroy', report_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report instance', new_report_instance_path %>
