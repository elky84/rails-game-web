<p id="notice"><%= notice %></p>

<h1>Listing Friend Instances</h1>

<table>
  <thead>
    <tr>
      <th>Friend no</th>
      <th>User no</th>
      <th>Friend user no</th>
      <th>State</th>
      <th>User send point date</th>
      <th>Friend user send point date</th>
      <th>User adv together date</th>
      <th>Friend user adv together date</th>
      <th>Is bot</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friend_instances.each do |friend_instance| %>
      <tr>
        <td><%= friend_instance.FRIEND_NO %></td>
        <td><%= friend_instance.USER_NO %></td>
        <td><%= friend_instance.FRIEND_USER_NO %></td>
        <td><%= friend_instance.STATE %></td>
        <td><%= friend_instance.USER_SEND_POINT_DATE %></td>
        <td><%= friend_instance.FRIEND_USER_SEND_POINT_DATE %></td>
        <td><%= friend_instance.USER_ADV_TOGETHER_DATE %></td>
        <td><%= friend_instance.FRIEND_USER_ADV_TOGETHER_DATE %></td>
        <td><%= friend_instance.IS_BOT %></td>
        <td><%= link_to 'Show', friend_instance %></td>
        <td><%= link_to 'Edit', edit_friend_instance_path(friend_instance) %></td>
        <td><%= link_to 'Destroy', friend_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friend instance', new_friend_instance_path %>
