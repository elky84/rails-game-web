<p id="notice"><%= notice %></p>

<h1>Listing Gacha Groups</h1>

<table>
  <thead>
    <tr>
      <th>Gacha group id</th>
      <th>Group reward</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gacha_groups.each do |gacha_group| %>
      <tr>
        <td><%= gacha_group.GACHA_GROUP_ID %></td>
        <td><%= gacha_group.GROUP_REWARD %></td>
        <td><%= gacha_group.DESCRIPTION %></td>
        <td><%= link_to 'Show', gacha_group %></td>
        <td><%= link_to 'Edit', edit_gacha_group_path(gacha_group) %></td>
        <td><%= link_to 'Destroy', gacha_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gacha group', new_gacha_group_path %>
