<h1>Listing daycheck_instances</h1>

<table>
  <thead>
    <tr>
      <th>Daycheck no</th>
      <th>User no</th>
      <th>User type</th>
      <th>Daycheck id</th>
      <th>Time</th>
      <th>Check</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daycheck_instances.each do |daycheck_instance| %>
      <tr>
        <td><%= daycheck_instance.DAYCHECK_NO %></td>
        <td><%= daycheck_instance.USER_NO %></td>
        <td><%= daycheck_instance.USER_TYPE %></td>
        <td><%= daycheck_instance.DAYCHECK_ID %></td>
        <td><%= daycheck_instance.TIME %></td>
        <td><%= daycheck_instance.CHECK %></td>
        <td><%= link_to 'Show', daycheck_instance %></td>
        <td><%= link_to 'Edit', edit_daycheck_instance_path(daycheck_instance) %></td>
        <td><%= link_to 'Destroy', daycheck_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daycheck instance', new_daycheck_instance_path %>
